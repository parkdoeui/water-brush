dius: 8px;\n  padding: 8px 16px;\n  color: #212121;\n  opacity: 0.8;\n}\n\n.button:hover {\n  opacity: 1;\n}\n\n\n.button:focus {\n  outline: 0;\n}\n\n.container {\n  position: absolute;\n  z-index: 1;\n  display: flex;\n  flex-direction: row;\n  justify-content: flex-end;\n}\n\n#red {\n  background: linear-gradient(128deg, rgba(255, 0, 215, 1) 0%, rgba(233, 0, 16, 1) 100%);\n}\n\n#green {\n  background: rgb(245, 255, 0);\n  background: linear-gradient(128deg, rgba(245, 255, 0, 1) 0%, rgba(95, 233, 0, 1) 100%);\n}\n\n#blue {\n  background: rgb(0, 176, 255);\n  background: linear-gradient(128deg, rgba(0, 176, 255, 1) 0%, rgba(0, 5, 233, 1) 100%);\n}\n\n#rainbow {\n  background: rgb(255, 0, 0);\n  background: linear-gradient(128deg, rgba(255, 0, 0, 1) 0%, rgba(233, 230, 0, 1) 30%, rgba(0, 228, 233, 1) 59%, rgba(233, 0, 99, 1) 93%);\n}\n\n.slider {\n  -webkit-appearance: none;\n  width: 100%;\n  height: 5px;\n  border-radius: 4px;\n  background: #d3d3d3;\n  outline: none;\n  opacity: 0.7;\n  -webkit-transition: .4s;\n  transition: opacity .4s;\n}\n\n.slider:hover {\n  opacity: 1;\n}\n\n\n.slider::-webkit-slider-thumb {\n  -webkit-appearance: none;\n  appearance: none;\n  width: 20px;\n  height: 20px;\n  border-radius: 50%;\n  background: #212121;\n  cursor: pointer;\n}\n\n.slider::-moz-range-thumb {\n  -webkit-appearance: none;\n  appearance: none;\n  width: 20px;\n  height: 20px;\n  border-radius: 50%;\n  background: #212121;\n  cursor: pointer;\n}"},"lineCount":188}},"error":null,"hash":"a51609c5721df625a10d86494ec5db3e","cacheData":{"env":{}}}